{
    "compilerOptions": {
        "target": "es6",
        "module": "es6",
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "moduleResolution": "Node",
        "declaration": true
    },
    "include": [
        "src/engine/**/*"
    ],
    "exclude": [
        "node_modules",
        "dist"
    ]
}
